{
  "name": "von-ts",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.2.1",
  "description": "Using tool types to ensuring type safety",
  "author": "ssshuai1999 <ssshuai1999@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ssshuai1999/von-ts#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssshuai1999/von-ts.git"
  },
  "bugs": "https://github.com/ssshuai1999/von-ts/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx unbuild",
    "play": "vite packages/playground",
    "dev": "npx unbuild --stub",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prepublishOnly": "nr build",
    "release": "npx bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.16.3",
    "@antfu/utils": "^0.5.2",
    "@types/node": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.19.0",
    "esno": "^0.16.3",
    "pnpm": "^7.5.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.14",
    "vitest": "^0.18.0"
  }
}
